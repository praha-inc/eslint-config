{
  "name": "@praha/eslint-config",
  "version": "1.0.0",
  "private": true,
  "description": "ESLint config for PrAha",
  "homepage": "https://github.com/praha-inc/eslint-config",
  "bugs": {
    "url": "https://github.com/praha-inc/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/praha-inc/eslint-config.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "lint:code": "turbo run lint:code",
    "lint:type": "turbo run lint:type",
    "version": "changeset",
    "version:apply": "changeset version",
    "version:publish": "changeset publish",
    "prepublish": "pkg-pr-new publish --pnpm --compact './packages/*'"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.2",
    "@changesets/cli": "2.29.8",
    "@tsconfig/strictest": "2.0.8",
    "pkg-pr-new": "0.0.63",
    "turbo": "2.8.3"
  },
  "packageManager": "pnpm@10.29.2"
}
